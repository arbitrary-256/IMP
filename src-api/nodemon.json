{
  "execMap": {
    "js": "node",
    "json": "node",
    "ts": "ts-node"
  },
  "extensions": [".js", ".json", ".ts"],
  "node": {
    "fs": "empty",
    "child_process": "empty",
    "net": "empty",
    "tls": "empty"
  },
  "watch": ["src", "impApi.ts"],
  "watchOptions": {
    "ignored": ["node_modules", "dist", "build", "coverage", "src-tauri", "__tests__", "__mocks__", "__fixtures__"],
    "poll": 1000
  }
}
